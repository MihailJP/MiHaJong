#defcfunc isDobon var GameStat, int targetPlayer
	exportScore GameStat, tmpArray
	tmpFlag = bignum_flag(tmpArray, targetPlayer)
	
	dobonFlag = 0
	if (tmpFlag == -1) {dobonFlag = 1}
